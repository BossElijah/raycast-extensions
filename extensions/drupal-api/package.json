{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "drupal-api",
  "title": "Drupal API",
  "description": "Search Drupal API documentation directly from Raycast",
  "icon": "512x512.png",
  "author": "BossElijah",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-drupal-api",
      "title": "Search Drupal API",
      "description": "Search Drupal API",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "cheerio": "^1.0.0-rc.12",
    "got": "^14.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/got": "^9.6.12",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
